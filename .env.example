# Copy this file to .env and fill in your actual values

# Slack Configuration
# Bot User OAuth Token from api.slack.com/apps
SLACK_BOT_TOKEN=xoxb-your-slack-bot-token-here
# App-Level Token for Socket Mode
SLACK_APP_TOKEN=xapp-1-your-app-token-here

# Atlassian Configuration
ATLASSIAN_API_EMAIL=your-email@example.com
# Generate at: id.atlassian.com/manage-profile/security/api-tokens
ATLASSIAN_API_TOKEN=your-api-token-here
# Your Atlassian instance URL
ATLASSIAN_URL=https://your-site.atlassian.net
# Cloud ID from admin.atlassian.com
ATLASSIAN_CLOUD_ID=your-cloud-id-here
# JIRA project key for incident tracking
JIRA_PROJECT_KEY=YOUR_KEY
# Confluence space ID for post-mortem pages
CONFLUENCE_SPACE_ID=YOUR_SPACE_ID

# AI Model Configuration  
# Choose: 'groq' (free & fast) or 'openai' (free $5 credits) or 'bedrock' (AWS required)
AI_MODEL_TYPE=groq
# Get free API key at: https://console.groq.com/keys
GROQ_API_KEY=gsk_your-groq-api-key-here
# Optional: Backup models
OPENAI_API_KEY=your-openai-api-key-here
AWS_BEARER_TOKEN_BEDROCK=your-aws-bedrock-token-here

# Status Page Configuration (optional)
STATUSPAGE_API_KEY=your-statuspage-api-key-here
STATUSPAGE_PAGE_ID=your-statuspage-page-id-here
